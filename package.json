{
  "name": "webpack-pug-ts-starter",
  "version": "1.0.0",
  "description": "A starter using pug, typescript, webpack and rxjs ",
  "main": "/src/index.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "npm run clean:dist && webpack",
    "build:prod": "npm run clean:dist && webpack",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "prodserver": "node prodserver",
    "start": "npm run start:dev",
    "start:dev": "npm run webdev",
    "start:prod": "npm run server:prod",
    "server:prod": "npm-run-all -p -r watch:prod prodserver",
    "rimraf": "rimraf",
    "webdev": "webpack-dev-server",
    "webpack": "npm run clean:dist && webpack",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\"",
    "watch:prod": "npm run build:prod -- --watch"
  },
  "author": "Adam Johannesmeyer <adam.johannesmeyer@gmail.com>",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/AdamJo/webpack-pug-ts-starter.git",
    "type": "git"
  },
  "dependencies": {
    "@types/node": "^6.0.45",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.0",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.0",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "raw-loader": "^0.5.1",
    "rxjs": "^5.0.0-rc.1",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "~1.1.1",
    "ts-loader": "~0.9.5",
    "ts-node": "~1.6.1",
    "tslint": "~3.15.1",
    "typescript": "~2.0.6",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "2.1.0-beta.4"
  }
}
